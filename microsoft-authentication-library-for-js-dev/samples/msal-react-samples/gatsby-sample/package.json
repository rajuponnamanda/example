{
  "name": "msal-react-gatsby-sample",
  "private": true,
  "description": "A basic sample for using msal-react with a Gatsby application",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "build:package": "cd ../../../lib/msal-react && npm run build:all",
    "develop": "gatsby develop -p 3000",
    "start": "gatsby serve -p 3000",
    "serve": "gatsby serve",
    "test": "jest",
    "clean": "gatsby clean",
    "install:local": "npm install ../../../lib/msal-react ../../../lib/msal-react/node_modules/react ../../../lib/msal-react/node_modules/react-dom ../../../lib/msal-browser",
    "install:published": "npm install @azure/msal-browser@latest @azure/msal-react@latest react@^18 react-dom@^18"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.14.2",
    "@azure/msal-react": "^1.0.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.1",
    "gatsby": "^4.0.0",
    "gatsby-material-ui-components": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^27.0.4"
  }
}
